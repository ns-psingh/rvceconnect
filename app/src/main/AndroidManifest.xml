<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.premal2.rvceconnect">

    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.DarkActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StudentFrontEnd" />
        <activity android:name=".AttendanceFrontEnd" />
        <activity android:name=".AboutActivity" />
        <activity
            android:name=".TeacherFrontEnd"
            android:label="@string/title_activity_teacher_front_end" />
        <activity android:name=".TeacherAttendanceUpdate" />
        <activity android:name=".counsellor_student_view" />
        <activity android:name=".TeacherStudentGaurdianContact" />
        <activity android:name=".StudentInfo" />
        <activity android:name=".AdminFrontEnd" />
        <activity android:name=".DeveloperInfo" />
        <activity android:name=".Usercreation" />
        <activity android:name=".FacingIssues" />
        <activity android:name=".AdminMessages"></activity>
    </application>

</manifest>